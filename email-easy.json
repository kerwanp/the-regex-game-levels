{
  "$schema": "./schema.json",
  "id": "email-easy",
  "name": "Email",
  "description": "Write a Regex that matches an email address",
  "difficulty": 1,
  "category": "Web",
  "tests": {
    "shouldMatch": [
      {
        "value": "example@gmail.com",
        "match": ["example", "gmail.com"]
      },
      {
        "value": "hello.world@hoo.example2.org",
        "match": ["hello.world", "hoo.example2.org"]
      },
      {
        "value": "no-please@the-regex-game.com",
        "match": ["no-please", "the-regex-game.com"]
      },
      {
        "value": "reply12@hellthread.net",
        "match": ["reply12", "hellthread.net"]
      }
    ],
    "shouldNotMatch": [
      {
        "value": "twiter.com/PaucotMartin"
      },
      {
        "value": "iamanemail"
      },
      {
        "value": "please@val_id.nop"
      },
      {
        "value": "this_does@not.work"
      },
      {
        "value": "idonthave@tld"
      },
      {
        "value": "@hello.world"
      },
      {
        "value": "this@should@not.work"
      }
    ]
  }
}
